<Page Name ="ListOpenCompetitions" x:Class="Ui.Main.Pages.OpenCompetitions.ListOpenCompetition"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Ui.Main.Pages.Competitions"
      xmlns:properties="clr-namespace:Ui.Main.Properties"
      mc:Ignorable="d" 
      d:DesignHeight="625" d:DesignWidth="1250"
      Title="ListOpenCompetitions" Height="545">
    <DockPanel  Height="545" VerticalAlignment="Bottom">
        <Label FontSize="18"
               FontWeight="Bold"
               HorizontalAlignment="Center"
                Content="{x:Static properties:Resources.TileCompetition}" DockPanel.Dock="Top"/>
        <DataGrid x:Name="DataGridCompetition" MinWidth="800" AutoGeneratingColumn="OnAutoGeneratingColumn" MinHeight="450"  VerticalAlignment="Top" ColumnWidth="*" Height="511" DockPanel.Dock="Bottom" IsReadOnly="True"/>
    </DockPanel>


</Page>
