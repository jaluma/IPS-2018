<mui:ModernDialog x:Class="Ui.Main.Pages.Inscriptions.Competitions.DialogPreinscripted"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:mui="http://firstfloorsoftware.com/ModernUI"
                  xmlns:payment="clr-namespace:Ui.Main.Pages.Inscriptions.Payment"
                  xmlns:properties="clr-namespace:Ui.Main.Properties"
                  mc:Ignorable="d"
                  d:DesignHeight="300" d:DesignWidth="300"
                  Title="{x:Static properties:Resources.FinishPreinscription}">


    <StackPanel>
        <Image Source="/Resources/Tick.png" HorizontalAlignment="Left" Height="127" Margin="82,32,0,0"
               VerticalAlignment="Top" Width="131" RenderTransformOrigin="0.3,-0.05" />
        <TextBlock Name="LbConfirmation" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center"
                   TextWrapping="Wrap" Width="272" Height="71" Margin="10,0" />
    </StackPanel>


    <!-- Note: ModernDialog buttons are defined in the constructor of the code behind class -->

</mui:ModernDialog>